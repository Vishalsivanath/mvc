@using Employe.Models
@model List<Employee>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetAllEmployee</title>
</head>
<body>
    <div>
        <table>
            <tr>
                <td>empid</td>
                <td>Name</td>
                <td>Gender</td>
                <td>Role</td>
                <td>Country</td>
                <td>State</td>
                <td>City</td>
                <td>Username</td>
                <td>Password</td>
                <td>Status</td>
            </tr>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@item.empid</td>
                    <td>@item.Name</td>
                    <td>@item.Gender</td>
                    <td>@item.Role></td>
                    <td>@item.Country</td>
                    <td>@item.State</td>
                    <td>@item.City</td>
                    <td>@item.Username</td>
                    <td>@item.Password</td>
                    <td>@item.Status</td>
                    <td>@Html.ActionLink("Delete","deleteEmployee",new {id =item.empid })</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
